<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CustomerList</servlet-name>
  	<display-name>CustomerList</display-name>
  	<description></description>
  	<servlet-class>com.example.CustomerList</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SuccessServlet</servlet-name>
  	<display-name>SuccessServlet</display-name>
  	<description></description>
  	<servlet-class>com.example.controller.SuccessServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>CustomerList</servlet-name>
  	<url-pattern>/Customer_List.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SuccessServlet</servlet-name>
  	<url-pattern>/success.do</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
  <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
